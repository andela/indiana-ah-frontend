  },
  "homepage": "https://github.com/andela/indiana-ah-frontend#readme",
  "dependencies": {
    "clean-webpack-plugin": "^2.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^24.3.1",
